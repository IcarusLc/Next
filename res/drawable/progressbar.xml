<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ffdddddd" android:endColor="#ffdddddd" android:angle="0.0" android:centerY="0.75" android:centerColor="#ffdddddd" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ffffff00" android:endColor="#a0ffcb00" android:angle="0.0" android:centerY="0.75" android:centerColor="#80ffb600" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ffffff00" android:endColor="#ffffff00" android:angle="0.0" />
            </shape>
        </clip>
    </item>
</layer-list>